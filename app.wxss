body,html,page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  background: #F9FBFE;
  font-family: Microsoft YaHei;
}
view{
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  font-family: Microsoft YaHei;
}
.viewWH{
  width: 100%;
  height: 100%;
  padding:40rpx 40rpx 0;
}
.entry{
  width: 100%;
  background: #FFFFFF;
  border-radius: 20rpx;
  padding: 0 20rpx;
  margin-bottom: 30rpx; 
}
#button{
  width: 100%;
  height: 88rpx;
  border-radius: 44rpx;
  background: #276DF5;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}

.buttonP{
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  height: 132rpx;
  background: #FFFFFF;
  box-shadow: 0px -4rpx 10rpx 0px #F7F7F7;
  display: flex;
    align-items: center;
justify-content: center;
padding:0 40rpx;
}

.buttonHover{
  opacity: 0.8;
}

.offline{
  border-bottom: 2rpx solid #F7F7F9;
}
