
.user{
  width: 100%;
  display: flex;
  align-items: center;
}
.user view:first-child{
  width: 120rpx;
height: 120rpx;
border-radius: 50%;
background: #ccc;
}
.user view:first-child image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.user view:last-child{
  font-size: 40rpx;
font-weight: 400;
color: #333333;
padding-left: 30rpx;
}
.my_list{
  padding-top: 60rpx;
}