
.detailsBox{
  width: 100%;
  background: #fff;
  border-radius: 20rpx;
  padding: 60rpx 30rpx;
}
.detailsBoxTime{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 40rpx;
}
.detailsBoxTime view:first-child{
  font-size: 22rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #999999;
}
.detailsBoxTime view:last-child{
  width: 87rpx;
height: 45rpx;
background: #FF838D;
border-radius: 8rpx;
display: flex;
align-items: center;
justify-content: center;
font-size: 22rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #FFFFFF;
}
.detailsBoxView{
  width: 100%;
  padding: 20rpx 0;
  font-size: 28rpx;
font-family: Microsoft YaHei;
font-weight: bold;
color: #333333;
}
.detailsBoxImage{
  width: 100%;
  display: flex;
  align-items: center;
}
.detailsBoxImage view{
  width: 140rpx;
height: 140rpx;
background: #FDD4D4;
border-radius: 20rpx;
margin-right: 40rpx;
}
.detailsBoxImage view image{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
}
