.ranking{
  padding-bottom: 40rpx;
}
.rankingView{
  width: 100%;
  height: 100%;
  overflow-y: auto;
  
}
.rankingViewList{
  width: 100%;
  padding: 0 30rpx;
  background: #FFFFFF;
}
.rankingViewListFor{
  width: 100%;
  padding: 30rpx;
  border-bottom: 2rpx solid #F7F7F9;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.rankingViewListForLeft{
  width: 80rpx;
height: 80rpx;
border-radius: 50%;
background: #ccc;
}
.rankingViewListForLeft image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.rankingViewListForRight{
  width: 78%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.rankingViewListForRight view:first-child{
  font-size: 28rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #333333;
}
.rankingText1{
  font-size: 30rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #333333;
}
.rankingText{
font-size: 36rpx;
font-family: Microsoft YaHei;
font-weight: bold;
color: #266CF4;
}