.choice{
  width: 100%;
  height: 88rpx;
background: #FFFFFF;
display: flex;
align-items: center;
justify-content: space-between;
padding: 0 30rpx;
}
.yes{
  border-radius: 44rpx;
  margin-bottom: 30rpx;
}
.no{
  border-bottom: 2rpx solid #F7F7F9;
}
.choiceViewFirst{
  font-size: 28rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #999999;
}
.choice .choiceView{
  font-size: 28rpx;
font-family: Microsoft YaHei;
font-weight: 400;
color: #999999;
}
.choice .choiceView .choiceViewText{
  font-size: 28rpx;
font-family: Microsoft YaHei;
font-weight: 400;

padding-right: 20rpx;
}
.choice .choiceView .images{
  width: 12rpx;
height: 22rpx;
}
.addHover{
  background-color: #C0C4CC;
}